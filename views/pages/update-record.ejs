<%- include('../partials/head') -%> <%- include('../partials/header') -%>
      <div class="container">
        <h1 class="text-center">Update the record</h1>
        <div class="row justify-content-md-center">
          <div class="col-">
            <form method="POST">

              <div class="form-group">
                <label for="name">Sales Person</label>
                <input
                  value="<%= record.salesperson %>"
                  type="text"
                  class="form-control"
                  id="salesperson"
                  name="salesperson"
                />
              </div>

              <div class="form-group">
                <label for="name">Customer Name</label>
                <input
                  value="<%= record.customername %>"
                  type="text"
                  class="form-control"
                  id="customername"
                  name="customername"
                />
              </div>

              <div class="form-group">
                <label for="name">Car Make</label>
                <input
                  value="<%= record.carmake %>"
                  type="text"
                  class="form-control"
                  id="carmake"
                  name="carmake"
                />
              </div>

              <div class="form-group">
                <label for="name">Car Model</label>
                <input
                  value="<%= record.carmodel %>"
                  type="text"
                  class="form-control"
                  id="carmodel"
                  name="carmodel"
                />
              </div>

              <div class="form-group">
                <label for="name">Car Year</label>
                <input
                  value="<%= record.caryear %>"
                  type="number"
                  class="form-control"
                  id="caryear"
                  name="caryear"
                />
              </div>

              <div class="form-group">
                <label for="name">Sale Price</label>
                <input
                  value="<%= record.saleprice %>"
                  type="number"
                  step="any"
                  class="form-control"
                  id="saleprice"
                  name="saleprice"
                />
              </div>

              <div class="form-group">
                <label for="name">Comission Rate</label>
                <input
                  value="<%= record.comissionrate %>"
                  type="number"
                  step="any"
                  class="form-control"
                  id="comissionrate"
                  name="comissionrate"
                />
              </div>
              
              <div class="form-group">
                <label for="name">Comission Earned</label>
                <input
                  value="<%= record.comissionearned %>"
                  type="number"
                  step="any"
                  class="form-control"
                  id="comissionearned"
                  name="comissionearned"
                />
 
              </div>
              

              <button type="submit" class="btn btn-primary">Submit</button>
            </form>
          </div>
        </div>
      </div>
      <%- include('../partials/footer') %>
    </body>
  </html>
</html>
